<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/dashboard_allover_layout_margin"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">


                    <LinearLayout
                        android:id="@+id/audit_layout"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/cell_size"
                        android:layout_margin="@dimen/cell_margin"
                        android:background="@drawable/report_border_layout"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/expandIcon"
                            android:layout_width="@dimen/icon_size"
                            android:layout_height="@dimen/icon_size"
                            android:src="@drawable/audit" />


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/audit"
                            android:gravity="center"
                            android:singleLine="true"
                            android:padding="@dimen/margin_5"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/layout_text_size" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/overall_brand_layout"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/cell_size"
                        android:layout_margin="@dimen/cell_margin"
                        android:gravity="center"
                        android:background="@drawable/report_border_layout"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/expandIcon2"
                            android:layout_width="@dimen/icon_size"
                            android:layout_height="@dimen/icon_size"
                            android:src="@mipmap/overall_brand" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/overallbrand"
                            android:gravity="center"
                            android:singleLine="true"
                            android:padding="@dimen/margin_5"
                            android:textAlignment="center"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/layout_text_size" />

                    </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">


                    <LinearLayout
                        android:id="@+id/detailed_summary_layout"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/cell_size"
                        android:layout_margin="@dimen/cell_margin"
                        android:gravity="center"
                        android:background="@drawable/report_border_layout"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/expandIcon3"
                            android:layout_width="@dimen/icon_size"
                            android:layout_height="@dimen/icon_size"
                            android:src="@drawable/self_assessment" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/detailedsummary"
                            android:gravity="center"
                            android:singleLine="true"
                            android:padding="@dimen/margin_5"
                            android:textAlignment="center"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/layout_text_size" />

                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/executive_summary_layout"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/cell_size"
                        android:layout_margin="@dimen/cell_margin"
                        android:gravity="center"
                        android:background="@drawable/report_border_layout"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/expandIcon4"
                            android:layout_width="@dimen/icon_size"
                            android:layout_height="@dimen/icon_size"
                            android:src="@mipmap/executive_summary" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/executivesummary"
                            android:gravity="center"
                            android:singleLine="true"
                            android:padding="@dimen/margin_5"
                            android:textAlignment="center"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/layout_text_size" />

                    </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/highlights_layout"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/cell_size"
                        android:layout_margin="@dimen/cell_margin"
                        android:gravity="center"
                        android:background="@drawable/report_border_layout"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/expandIcon5"
                            android:layout_width="@dimen/icon_size"
                            android:layout_height="@dimen/icon_size"
                            android:src="@mipmap/internal_audit" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/highlights"
                            android:gravity="center"
                            android:singleLine="true"
                            android:padding="@dimen/margin_5"
                            android:textColor="@color/colorWhite"

                            android:textSize="@dimen/layout_text_size" />

                    </LinearLayout>



                    <LinearLayout
                        android:id="@+id/audio_image_layout"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/cell_size"
                        android:layout_margin="@dimen/cell_margin"
                        android:gravity="center"
                        android:background="@drawable/report_border_layout"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/expandIcon6"
                            android:layout_width="@dimen/icon_size"
                            android:layout_height="@dimen/icon_size"
                            android:src="@drawable/audio_image_icon" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/audio_images"
                            android:gravity="center"
                            android:singleLine="true"
                            android:padding="@dimen/margin_5"
                            android:layout_marginBottom="10dp"
                            android:textAlignment="center"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/layout_text_size" />

                    </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/cell_size"
                android:layout_margin="@dimen/cell_margin"
                android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/back_house_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:background="@drawable/report_border_layout"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/expandIcon7"
                            android:layout_width="@dimen/icon_size"
                            android:layout_height="@dimen/icon_size"
                            android:src="@mipmap/internal_audit" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/back_house"
                            android:textColor="@color/colorWhite"
                            android:gravity="center"
                            android:singleLine="true"
                            android:padding="@dimen/margin_5"
                            android:textSize="@dimen/layout_text_size" />

                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/integrity_layout"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/cell_size"
                        android:layout_margin="@dimen/cell_margin"
                        android:gravity="center"
                        android:background="@drawable/report_border_layout"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/expandIcon8"
                            android:layout_width="@dimen/icon_size"
                            android:layout_height="@dimen/icon_size"
                            android:src="@mipmap/integrity_icon" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/integrity"
                            android:gravity="center"
                            android:singleLine="true"
                            android:paddingBottom="@dimen/margin_5"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/layout_text_size" />

                    </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:orientation="vertical">



                    <LinearLayout
                        android:id="@+id/faq_layout"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/cell_size"
                        android:layout_margin="@dimen/cell_margin"
                        android:gravity="center"
                        android:background="@drawable/report_border_layout"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/expandIcon9"
                            android:layout_width="@dimen/icon_size"
                            android:layout_height="@dimen/icon_size"
                            android:src="@mipmap/internal_audit" />

                        <TextView
                            android:id="@+id/tv_faq_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/padding_5"
                            android:gravity="center"
                            android:singleLine="true"
                            android:paddingBottom="@dimen/margin_5"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/layout_text_size" />

                    </LinearLayout>



                    <!--<LinearLayout
                        android:id="@+id/star_employee_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:background="@drawable/report_border_layout"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/expandIcon10"
                            android:layout_width="@dimen/icon_size"
                            android:layout_height="@dimen/icon_size"
                            android:src="@drawable/integrity_icon" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/integrity"
                            android:layout_marginBottom="10dp"
                            android:textAlignment="center"
                            android:textColor="@color/colorWhite"
                            android:textSize="@dimen/layout_text_size" />

                    </LinearLayout>-->



            </LinearLayout>

        </LinearLayout>

    </ScrollView>

</LinearLayout>